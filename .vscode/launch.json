{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Launch via npm",
      "request": "launch",
      "runtimeArgs": [
          "run",
          "start:dev"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.debug.env",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
    },
  ]
}
